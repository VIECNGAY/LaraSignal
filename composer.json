            "name": "Quyet Tran",
            "email": "quyettv.it@gmail.com",
            "homepage": "topcv.vn"
        }
    ],
    "homepage": "https://github.com/viecngay/larasignal",
    "keywords": ["Laravel", "LaraSignal"],
    "require": {
        "guzzlehttp/guzzle": "~6.*",
        "illuminate/support": "~5"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3.0",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "ViecNgay\\LaraSignal\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ViecNgay\\LaraSignal\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ViecNgay\\LaraSignal\\LaraSignalServiceProvider"
            ],
            "aliases": {
                "LaraSignal": "ViecNgay\\LaraSignal\\Facades\\LaraSignal"
            }
        }
    }
}
